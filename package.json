{
  "name": "snackables",
  "version": "0.0.6",
  "description": "Loading byte sized ENVs made simple.",
  "main": "lib/index.js",
  "sideEffects": false,
  "homepage": "https://github.com/mattcarlotta/snackables",
  "files": [
    "lib/config.js",
    "lib/extract.js",
    "lib/index.js",
    "lib/log.js",
    "lib/parse.js",
    "lib/set.js",
    "register.js",
    "types"
  ],
  "types": "types/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dtslint": "dtslint types",
    "ex:basic": "ENV_LOAD=test ENV_DEBUG=true ts-node examples/basic.ts",
    "ex:custom": "ts-node examples/custom.ts",
    "ex:preload": "ENV_LOAD=test ENV_DEBUG=true ts-node -r snackables/register examples/preload.ts",
    "ex:snackable": "ENV_LOAD=base,test ENV_DEBUG=true ts-node examples/snackable.ts",
    "lint": "eslint ./ --ext .ts,.js",
    "test": "jest --config ./jest.json",
    "test:cov": "jest --config ./jest.json --coverage",
    "test:watch": "jest --config ./jest.json --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+git@github.com:mattcarlotta/snackables.git"
  },
  "keywords": [
    "snackables",
    "env",
    ".env",
    "environment",
    "variables",
    "config",
    "settings"
  ],
  "author": "Matt Carlotta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattcarlotta/snackables/issues"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.7",
    "@types/jest": "^26.0.12",
    "@types/node": "^14.6.4",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "browserslist": "^4.14.0",
    "dtslint": "^4.0.4",
    "enzyme": "^3.11.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "snackables": "^0.0.6",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {}
}
