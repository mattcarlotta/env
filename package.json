{
  "name": "snackables",
  "version": "0.0.21",
  "description": "Loading byte sized ENVs made simple.",
  "main": "lib/index.js",
  "sideEffects": false,
  "homepage": "https://github.com/mattcarlotta/snackables",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && ts-node compress.ts",
    "ex:arrpaths": "ts-node examples/arrpaths.ts",
    "ex:basic": "ENV_LOAD=test ENV_DEBUG=true ts-node examples/basic.ts",
    "ex:config": "ENV_DEBUG=true ts-node examples/config.ts",
    "ex:interpolate": "ts-node examples/interpolate.ts",
    "ex:parse": "ts-node examples/parse.ts",
    "ex:path": "ts-node examples/path.ts",
    "ex:preload": "ENV_LOAD=test ENV_DEBUG=true ts-node -r snackables examples/preload.ts",
    "ex:preloadcustompaths": "ENV_LOAD=tests/.env.base,tests/.env.test ENV_DEBUG=true ts-node -r snackables examples/preloadcustompaths.ts",
    "ex:preloadpaths": "ENV_LOAD=.env.base,.env.test ENV_DEBUG=true ts-node -r snackables examples/preloadpaths.ts",
    "ex:strpaths": "ts-node examples/strpaths.ts",
    "examples": "npm run ex:arrpaths && npm run ex:basic && npm run ex:config && npm run ex:interpolate && npm run ex:parse && npm run ex:path && npm run ex:preload && npm run ex:preloadcustompaths && npm run ex:preloadpaths && npm run ex:strpaths",
    "lint": "eslint ./ --ext .ts,.js",
    "test": "jest --config ./jest.json",
    "test:cov": "jest --config ./jest.json --coverage",
    "test:watch": "jest --config ./jest.json --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+git@github.com:mattcarlotta/snackables.git"
  },
  "keywords": [
    "snackables",
    "env",
    ".env",
    "environment",
    "variables",
    "config",
    "settings"
  ],
  "author": "Matt Carlotta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattcarlotta/snackables/issues"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.7",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.2",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "browserslist": "^4.14.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.0",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "snackables": "^0.0.20",
    "terser": "^5.3.8",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {}
}
