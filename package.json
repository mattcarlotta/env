{
  "name": "snackables",
  "version": "2.0.1",
  "description": "Loading byte sized ENVs made simple.",
  "main": "lib/index.js",
  "sideEffects": false,
  "homepage": "https://github.com/mattcarlotta/snackables",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc && ts-node compress.ts",
    "lint": "eslint ./ --ext .ts,.js",
    "test": "rimraf index.js && jest --config ./jest.json",
    "test:cov": "rimraf index.js && jest --config ./jest.json --coverage",
    "test:watch": "rimraf index.js && jest --config ./jest.json --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+git@github.com:mattcarlotta/snackables.git"
  },
  "keywords": [
    "snackables",
    "env",
    ".env",
    "environment",
    "variables",
    "config",
    "settings"
  ],
  "author": "Matt Carlotta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattcarlotta/snackables/issues"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "browserslist": "^4.14.7",
    "enzyme": "^3.11.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "terser": "^5.3.8",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {}
}
