{
  "name": "snackables",
  "version": "3.0.0",
  "description": "Loading byte sized ENVs made simple.",
  "main": "lib/index.js",
  "sideEffects": false,
  "homepage": "https://github.com/mattcarlotta/snackables",
  "files": [
    "index.js",
    "config/index.js",
    "parse/index.js",
    "load/index.js",
    "types/index.d.ts"
  ],
  "types": "types/index.d.ts",
  "scripts": {
    "b": "tsc --p ./tsconfig.cjs.json && ts-node ./utils/compress.ts",
    "b:esm": "tsc --p ./tsconfig.esm.json",
    "l": "eslint ./ --ext .ts,.js",
    "t": "rimraf index.js && jest --config ./jest.json",
    "t:cov": "rimraf index.js && jest --config ./jest.json --coverage",
    "t:watch": "rimraf index.js && jest --config ./jest.json --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+git@github.com:mattcarlotta/snackables.git"
  },
  "keywords": [
    "snackables",
    "env",
    ".env",
    "environment",
    "variables",
    "config",
    "settings"
  ],
  "author": "Matt Carlotta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattcarlotta/snackables/issues"
  },
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "browserslist": "^4.16.6",
    "enzyme": "^3.11.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.1",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "terser": "^5.7.0",
    "ts-jest": "^27.0.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {}
}
